platform :ios, '13.0'
use_frameworks!

target 'Bitcoin Adventurer' do
  pod 'Swinject'
  pod 'SwinjectAutoregistration'  # Very important as allows for autoregister of types in container
  pod 'SwinjectStoryboard' # Very important as it allows dependency injections into UIViewControllers
  
  target 'Bitcoin Adventurer Tests' do
    pod 'Swinject'
    pod 'SwinjectAutoregistration'
  end
  
end

